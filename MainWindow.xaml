<Window x:Class="AlarmClock.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AlarmClock"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Name="currentTimeLabel" Content="00:00:00" HorizontalAlignment="Center" Margin="0,130,0,0" VerticalAlignment="Top" FontSize="48"/>
        <TextBox Name="alarmTextBox" HorizontalAlignment="Center" Margin="0,289,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Name="setButton" Content="Set" HorizontalAlignment="Left" Margin="601,287,0,0" VerticalAlignment="Top" Click="setButton_Click" Height="21" Width="64"/>
        <Label Content="Alartime:" HorizontalAlignment="Left" Margin="225,285,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
